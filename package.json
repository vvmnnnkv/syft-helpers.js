{
  "name": "syft-helpers.js",
  "version": "0.0.1",
  "description": "A simple helpers library for syft.js and grid.js",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.js",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OpenMined/syft-helpers.js.git"
  },
  "keywords": [
    "syft",
    "helpers",
    "pysyft",
    "openmined",
    "open",
    "mined",
    "deep",
    "learning",
    "private",
    "javascript",
    "machine",
    "learning"
  ],
  "author": "OpenMined",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/OpenMined/syft-helpers.js/issues"
  },
  "homepage": "https://github.com/OpenMined/syft-helpers.js#readme",
  "scripts": {
    "start": "rollup -cw",
    "build": "rollup -c",
    "prepare": "npm run build",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "browserslist": "> 0.25%, not dead",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/src/_errors.js",
      "<rootDir>/test/dummy"
    ]
  },
  "dependencies": {
    "immutable-tuple": "^0.4.10",
    "pysyft-proto": "git+https://github.com/vvmnnnkv/proto.git#serde-protocol"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-jest": "^24.8.0",
    "husky": "^3.0.1",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "rollup": "1.20.2",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-filesize": "^6.1.1",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.0"
  }
}
